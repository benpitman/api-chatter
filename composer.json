{
    "name": "benpitman/api-chatter",
    "description": "A universal chat system",
    "authors": [
        {
            "name": "Ben Pitman"
        }
    ],
    "require": {
        "pitmanware/kentron": "^4.0",
        "pitmanware/kentron-slim": "^4.0",
        "pitmanware/kentron-illuminate": "^4.0",
        "pitmanware/kentron-phpmailer": "^4.0",
        "symfony/console": "^6.0"
    },
    "require-dev": {
        "spiral/dumper": "*",
        "nyholm/psr7-server": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "preferred-install": "source"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
